# .env - Production Environment Variables
# CRITICAL: DO NOT commit this file to version control.
# Copy this to .env on your production server and fill in the values.

# PostgreSQL Database
DB_NAME=mentorship_portal_prod
DB_USER=postgres
DB_PASSWORD=YOUR_STRONG_DATABASE_PASSWORD_HERE

# Backend JWT Secret
JWT_SECRET=YOUR_VERY_STRONG_JWT_SECRET_KEY_HERE

# Redis Password (if using Redis in production)
REDIS_PASSWORD=YOUR_STRONG_REDIS_PASSWORD_HERE

# Application Port (should match your firewall settings)
PORT=3001

# Node Environment
NODE_ENV=production
